{
  "name": "shatabang-server",
  "version": "0.8.1",
  "description": "Server script for the Shatabang Picture library managing tools",
  "author": "Andreas Sehr <andreas@sehr.se>",
  "contributors": [],
  "bin": {
    "module-name": ""
  },
  "scripts": {
    "dev": "npm-run-all --parallel start_redis server",
