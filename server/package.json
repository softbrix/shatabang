    "bull": "3.20.1",
    "bull-arena": "3.11.0",
    "bull-board": "1.4.1",
    "compression": "1.7.4",
    "connect-redis": "3.4.2",
    "express": "4.17.1",
    "express-session": "1.16.2",
    "fs-extra": "8.1.0",
    "idre-array-cache": "0.3.1",
    "multer": "1.4.1",
    "node-dir": "0.1.17",
    "passport": "0.4.0",
    "passport-google-oauth20": "1.0.0",
    "passport-local": "1.0.0",
    "redis": "3.0.2",
