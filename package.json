{
  "name": "shatabang",
  "version": "0.7.2",
  "description": "Picture library managing tools",
  "author": "Andreas Sehr <andreas@sehr.se>",
  "contributors": [],
  "bin": {
    "module-name": ""
  },
  "scripts": {
    "dev": "npm-run-all --parallel start_redis run_server run_taskprocessor",
    "test": "mocha",
    "start": "npm-run-all --parallel run_server run_taskprocessor",
    "start_redis": "redis-server",
    "run_server": "nodemon",
    "run_taskprocessor": "node task_processor.js",
    "build_client": "cd client && ember build --environment='production'",
    "client_install": "cd client && npm install;"
  },
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/softbrix/shatabang"
  },
  "bugs": {
    "url": "https://github.com/softbrix/shatabang/issues"
  },
  "keywords": [
    "pictures",
    "gallery",
    "shatabang",
    "softbrix",
    "opencv",
    "docker",
    "ember",
    "node"
  ],
  "dependencies": {
    "async": "1.5.2",
    "babel-preset-react": "6.24.1",
    "body-parser": "1.18.2",
    "bull": "3.7.0",
    "bull-arena": "2.6.2",
    "compression": "1.7.4",
    "connect-redis": "3.4.1",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "fluent-ffmpeg": "2.1.2",
    "fs-extra": "*",
    "googleapis": "^39.2.0",
    "minimist": "1.2.0",
    "multer": "1.4.1",
    "node-dir": "*",
    "node-sha1": "1.0.1",
    "npm-run-all": "4.1.5",
    "obj-flatten": "2.0.4",
    "opencv4nodejs": "4.14.1",
    "passport": "0.4.0",
    "passport-google-oauth20": "1.0.0",
    "passport-local": "1.0.0",
    "progress": "1.1.8",
    "q": "1.5.1",
    "request": "2.88.0",
    "request-promise": "4.2.4",
    "sha1-file": "1.0.0",
    "sha256": "0.2.0",
    "sharp": "0.21.3",
    "sharp-phash": "1.0.2",
    "stureby_index": "3.0.0",
    "underscore": "*",
    "uuid": "3.3.2",
    "variance": "0.0.1",
    "vega_media_info": "2.4.2",
    "vemdalen_index": "1.4.2"
  },
  "devDependencies": {
    "fakeredis": "2.0.0",
    "mocha": "6.1.4",
    "nodemon": "^1.18.11"
  },
  "preferGlobal": true,
  "private": true,
  "subdomain": "shatabang",
  "analyze": true,
  "license": "MIT"
}
