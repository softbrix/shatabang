{
  "name": "shatabang",
  "version": "0.7.16",
  "description": "Picture library managing tools",
  "author": "Andreas Sehr <andreas@sehr.se>",
  "contributors": [],
  "bin": {
    "module-name": ""
  },
  "scripts": {
    "dev": "npm-run-all --parallel start_redis run_server run_taskprocessor",
    "test": "mocha",
    "start": "npm-run-all --parallel run_server run_taskprocessor",
    "start_redis": "redis-server",
    "run_server": "nodemon",
    "run_taskprocessor": "nodemon task_processor.js",
    "build_client": "cd client && ember build --environment='production'",
    "client_install": "cd client && npm install;"
  },
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/softbrix/shatabang"
  },
  "bugs": {
    "url": "https://github.com/softbrix/shatabang/issues"
  },
  "keywords": [
    "pictures",
    "gallery",
    "shatabang",
    "softbrix",
    "opencv",
    "docker",
    "ember",
    "node"
  ],
  "dependencies": {
    "async": "1.5.2",
    "body-parser": "1.19.0",
    "bull": "3.10.0",
    "bull-arena": "2.6.3",
    "compression": "1.7.4",
    "connect-redis": "3.4.2",
    "express": "4.17.1",
    "express-session": "1.16.2",
    "fluent-ffmpeg": "2.1.2",
    "fs-extra": "8.1.0",
    "googleapis": "47.0.0",
    "minimist": "1.2.0",
    "multer": "1.4.1",
    "node-dir": "0.1.17",
    "node-sha1": "1.0.1",
    "obj-flatten": "2.0.4",
    "opencv4nodejs": "4.17.0",
    "passport": "0.4.0",
    "passport-google-oauth20": "1.0.0",
    "passport-local": "1.0.0",
    "request": "2.88.0",
    "request-promise": "4.2.4",
    "sha1-file": "1.0.4",
    "sha256": "0.2.0",
    "sharp": "0.22.1",
    "sharp-phash": "1.0.2",
    "stureby-index": "3.1.4",
    "underscore": "1.9.1",
    "uuid": "3.3.2",
    "variance": "0.0.1",
    "vega-media-info": "2.5.3",
    "vemdalen-index": "1.5.0"
  },
  "devDependencies": {
    "fakeredis": "2.0.0",
    "mocha": "6.2.0",
    "npm-run-all": "4.1.5",
    "nodemon": "1.19.1"
  },
  "preferGlobal": true,
  "private": true,
  "subdomain": "shatabang",
  "analyze": true,
  "license": "MIT"
}
