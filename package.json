{
  "name": "shatabang",
  "version": "0.5.0",
  "description": "Picture library managing tools",
  "author": "Andreas Sehr <andreas@sehr.se>",
  "contributors": [],
  "bin": {
    "module-name": ""
  },
  "scripts": {
    "dev": "npm-run-all --parallel start_redis run_server run_taskprocessor",
    "test": "mocha",
    "start": "npm-run-all --parallel run_server run_taskprocessor",
    "start_redis": "redis-server",
    "run_server": "node server.js",
    "run_taskprocessor": "node task_processor.js",
    "build_client": "cd client && ember build --environment='production'",
    "postinstall": "cd client && npm install;"
  },
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/softbrix/shatabang"
  },
  "bugs": {
    "url": "https://github.com/softbrix/shatabang/issues"
  },
  "keywords": [
    "pictures",
    "gallery",
    "shatabang",
    "softbrix",
    "opencv",
    "docker",
    "ember",
    "node"
  ],
  "dependencies": {
    "async": "^1.5.2",
    "babel-preset-react": "^6.5.0",
    "body-parser": "^1.15.0",
    "compression": "^1.6.1",
    "connect-redis": "3.3.3",
    "express": "4.13.3",
    "express-session": "^1.13.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "*",
    "kue": "^0.11.6",
    "minimist": "^1.2.0",
    "multer": "1.1.0",
    "node-dir": "*",
    "node-sha1": "^1.0.1",
    "npm-run-all": "^4.0.2",
    "opencv4nodejs": "^2.38.0",
    "passport": "^0.3.2",
    "passport-google-oauth20": "1.0.0",
    "passport-local": "1.0.0",
    "progress": "1.1.8",
    "q": "^1.4.1",
    "sha1-file": "^1.0.0",
    "sha256": "0.2.0",
    "sharp": "^0.18.4",
    "sharp-phash": "^0.2.0",
    "stureby_index": "3.0.0",
    "underscore": "*",
    "uuid": "^3.2.1",
    "variance": "^0.0.1",
    "vega_media_info": "^1.2.0"
  },
  "devDependencies": {
    "mocha": "~2.3.3"
  },
  "preferGlobal": true,
  "private": true,
  "subdomain": "shatabang",
  "analyze": true,
  "license": "MIT"
}
