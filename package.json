{
  "name": "shatabang",
  "version": "0.8.1",
  "description": "Picture library managing tools",
  "author": "Andreas Sehr <andreas@sehr.se>",
  "contributors": [],
  "bin": {
    "module-name": ""
  },
  "scripts": {
    "dev": "npm-run-all --parallel start_redis server",
    "test": "mocha",
    "start": "node --preserve-symlinks server/server.js",
    "start_redis": "redis-server",
    "server": "nodemon",
    "build_client": "cd client && ember build --environment='production'",
    "client_install": "cd client && npm install;"
  },
  "main": "server/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/softbrix/shatabang"
  },
  "bugs": {
    "url": "https://github.com/softbrix/shatabang/issues"
  },
  "keywords": [
    "pictures",
    "gallery",
    "shatabang",
    "softbrix",
    "opencv",
    "docker",
    "ember",
    "node"
  ],
  "dependencies": {
    "bull": "3.10.0",
    "connect-redis": "3.4.2",
    "idre-array-cache": "0.2.3",
    "fs-extra": "8.1.0",
    "node-dir": "0.1.17",
    "redis": "^3.0.2",
    "sha256": "0.2.0",
    "sharp": "0.25.4",
    "stureby-index": "3.1.4",
    "underscore": "1.9.1",
    "vemdalen-index": "1.5.0"
  },
  "devDependencies": {
    "fakeredis": "2.0.0",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "4.1.5"
  },
  "preferGlobal": true,
  "private": true,
  "subdomain": "shatabang",
  "analyze": true,
  "license": "MIT"
}
